{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./src",
    "rootDir": "./dev",
    "watch": true,
    "alwaysStrict": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "pretty": true,
    "strictNullChecks": true
  }
}